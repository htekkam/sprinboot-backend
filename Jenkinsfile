node {
    

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                echo 'this is build stage'
            }
           
            }
        stage('Compile') {
            steps {
                // Get some code from a GitHub repository
                echo 'this is compile stage'
            }

           
            }
         stage('document') {
            steps {
                // Get some code from a GitHub repository
                echo 'this is document stage'
            }

            }
        }
    }

